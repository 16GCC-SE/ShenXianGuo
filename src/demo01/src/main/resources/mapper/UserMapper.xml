<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.gcc.course.springbootdao.UserMapper">
    <select id="getAll" resultType="cn.gcc.course.springboot.model.User">
        select id,subject,A,B,C,D,answer,img from user
    </select>

    <select id="findById" parameterType="BigInteger" resultType="cn.gcc.course.springboot.model.User">
        select id,subject,A,B,C,D,answer,img from user WHERE id=#{id}
    </select>

